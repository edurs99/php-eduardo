public class IsiCarrosLoja {
	public static void main(String[] args) {
		// declarando variaveis tipo carro;
		Carro c1, c2;
		// Aqui efetivamente eu peco memoria para JVM para poder armazenar os dados.
		c1 = new Carro();
		c2 = new Carro();
		//criando os itens
		
		c1.marca = "Porshe";
		c1.modelo = "Cayenne";
		c1.cor = "Azul Marinho Nervoso";
		c1.ano = 2020;
		c1.preco = 1250000.00;
		
		c2.marca = "Xevrole";
		c2.modelo = "Corsa";
		c2.cor = "Prata com detalhes brancos";
		c2.ano = 1998;
		c2.preco = 3852.00;
		
		System.out.println("Carro");
	}
}
