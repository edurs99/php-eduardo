package Exercicio_Cartao;

public class Balada {
	public static void main(String[] args) {
//		CartaoPrePago c1 c2;
		CartaoPrePago c1 = new CartaoPrePago("123445", "Joao Carlos", 2025, 06, 10000);
		CartaoCashBack c2 = new CartaoCashBack("11211", "Elias Maluco", 2024, 6, 5000, 0);
		System.out.println(c1);
		System.out.println(c2);
		
		c1.adicionarCredito(2000);
		System.out.println(c1);
		
		c1.comprar(3000000);
		System.out.println(c1);
		
	}
}
